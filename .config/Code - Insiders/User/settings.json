{
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter-notebook",
            "filenamePattern": "*.ipynb"
        }
    ],
    "jupyter.insidersChannel": "weekly",
    "python.insidersChannel": "weekly",
    "python.defaultInterpreterPath": "",
    "jupyter.alwaysTrustNotebooks": true,
    "jupyter.askForKernelRestart": false,
    "jupyter.showKernelSelectionOnInteractiveWindow": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "python.formatting.provider": "black",
    "python.linting.pylintEnabled": false,
    "debug.openDebug": "openOnDebugBreak",
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false,
    },
    "autoDocstring.docstringFormat": "numpy",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.configuration.checkProjectSettingsExclusions": false,
    "python.languageServer": "Pylance",
    "jupyter.searchForJupyter": false,
    "notebook.kernelProviderAssociations": [
        {
            "viewType": "jupyter-notebook",
            "kernelProvider": "ms-toolsai.jupyter"
        }
    ],
    "python.linting.flake8Enabled": true,
    "git.enableSmartCommit": true,
    "jupyter.themeMatplotlibPlots": true,
    "python.terminal.activateEnvironment": false,
    "window.autoDetectColorScheme": true,
    "workbench.iconTheme": "Monokai Pro Icons",
    "workbench.colorTheme": "Monokai Pro",
    "explorer.autoReveal": false,
    "workbench.startupEditor": "newUntitledFile",
    "jupyter.widgetScriptSources": [
        "jsdelivr.com",
        "unpkg.com"
    ],
    "update.mode": "none",
    "files.watcherExclude": {
        "**/*-env/**": true,
        "**/data/**": true
    },
    "[python]": {
        "editor.rulers": [
            88
        ]
    },
    "latex-utilities.message.update.show": false,
    "latex-workshop.message.update.show": false,
    "latex-workshop.view.pdf.viewer": "tab",
    "rewrap.autoWrap.enabled": false,
    "diffEditor.wordWrap": "off",
    "terminal.external.linuxExec": "konsole",
    "redhat.telemetry.enabled": false,
    "dart.checkForSdkUpdates": false,
    "hediet.vscode-drawio.local-storage": "",
    "hediet.vscode-drawio.codeLinkActivated": true,
    "git.terminalAuthentication": false,
    "http.proxyAuthorization": null,
}